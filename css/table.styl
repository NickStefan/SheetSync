.table-container
  position fixed
  top 134px
  left 245px
  #spreadsheet
    height calc(100vh - 164px)
    width calc(100vw - 245px)
  .sheet-select
  border none
  font-family Quicksand
  font-size 16px
  li
    font-weight bold
    a:hover
      color black
  li.active
    border-bottom 1px solid light-green
    a
      border none
      color white
      background-color light-green
    a:hover
      border none
      background-color light-green
      color white
  .nav
    li
      a
        padding-top: 6px
